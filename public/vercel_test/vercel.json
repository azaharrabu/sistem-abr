{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "rujukan_interaktif_testing.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "server.js" },
    { "src": "/rujukan_interaktif_testing.html", "dest": "rujukan_interaktif_testing.html" },
    { "src": "/(.*)", "dest": "rujukan_interaktif_testing.html" }
  ]
}